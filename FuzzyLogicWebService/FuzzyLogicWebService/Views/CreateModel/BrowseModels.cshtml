@model FuzzyLogicWebService.FISFiles.DBModel.User

@{
    ViewBag.Title = "BrowseModels";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.UserName, these are your models</h2>
<table border="1" style="width:1000px">

        <tr>
          <td><b>Name</b></td>
          <td><b>Description</b></td>
          <td><b>Number of inputs variables</b></td>
          <td><b>Number of output variables</b></td>
          <td><b>Number of rules</b></td>
        </tr>

    @foreach (var m in Model.Models)
    {
        <tr>
          <td><b>@m.Name</b></td>
          <td>@m.Description</td>
          <td>@m.InputsNumber</td>
          <td>@m.OutputsNumber</td>
          <td>@m.RulesNumber</td>
          <td>@Html.ActionLink("Generate .fis file","SaveFileLocally", "Home",null,new {model = m})</td>
        </tr>
    }
</table>